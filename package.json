{
  "name": "dsf-wegbot2",
  "version": "2.0.1",
  "description": "Modbot for Dallas Sports Fam discord",
  "license": "GPL-3.0",
  "author": "Shawn Lutch <shawn@chaoticweg.cc> (https://chaoticweg.cc)",
  "private": true,
  "scripts": {
    "build": "npx tsc && gulp copy",
    "clean": "npx rimraf lib",
    "lint": "npx tslint --project .",
    "restart": "npx pm2 restart ecosystem.config.js --update-env",
    "start": "npx pm2 start ecosystem.config.js",
    "status": "npx pm2 status ecosystem.config.js",
    "stop": "npx pm2 stop ecosystem.config.js",
    "test": "yarn run clean && yarn run lint && yarn run build && node lib/index.js"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "discord.js": "^11.4.2",
    "dotenv": "^7.0.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "pm2": "^3.5.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.6",
    "coveralls": "^3.0.3",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.1.0",
    "jest": "^24.5.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/events/",
      "/lib/"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
