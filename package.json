{
  "name": "dsf-wegbot2",
  "version": "2.0.0",
  "description": "Modbot for Dallas Sports Fam discord",
  "license": "GPL-3.0",
  "author": "Shawn Lutch <shawn@chaoticweg.cc> (https://chaoticweg.cc)",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "build": "yarn run build:types && yarn run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline --copy-files",
    "lint": "tslint --project .",
    "test": "jest --setupFiles dotenv/config",
    "test:coverage": "jest --coverage --setupFiles dotenv/config",
    "test:ci": "yarn run lint && yarn run test:coverage && cat ./coverage/lcov.info | coveralls",
    "start": "node lib/index.js"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "discord.js": "^11.4.2",
    "dotenv": "^7.0.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.3",
    "babel-jest": "^24.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.3",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/events/"
    ]
  }
}
